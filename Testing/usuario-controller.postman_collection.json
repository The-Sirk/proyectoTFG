	"item": [
		{
			"name": "Post /usuarios/crearUsuario",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Codigo de estado 201, usuario creado\", function () {\r",
							"    if(pm.response == 201){\r",
							"     pm.response.to.have.status(201);\r",
							"    }\r",
							"});\r",
							"\r",
							"var json = pm.response.json();\r",
							"\r",
							"pm.environment.set(\"documentID\", json.documentID);\r",
							"pm.environment.set(\"correo\", json.correo);\r",
							"pm.environment.set(\"imagen_perfil\", json.imagen_perfil);\r",
							"pm.environment.set(\"nick\", json.nick);\r",
							"pm.environment.set(\"amigos_id\", json.amigos_id);\r",
							"pm.environment.set(\"peliculas_criticadas\", json.peliculas_criticadas);\r",
							"pm.environment.set(\"peliculas_favoritas\", json.peliculas_favoritas);\r",
							"pm.environment.set(\"peliculas_vistas\", json.peliculas_vistas);\r",
							"\r",
							"pm.test(`Usuario creado - documentID: ${json.documentID} - nick: ${json.nick}`)"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n  \"correo\": \"{{correo}}\",\r\n  \"imagen_perfil\": \"{{imagen_perfil}}\",\r\n  \"nick\": \"{{nick}}\",\r\n  \"amigos_id\": [{{amigos_id}}],\r\n  \"peliculas_criticadas\": [\r\n    {{peliculas_criticadas}}\r\n  ],\r\n  \"peliculas_favoritas\": [\r\n   {{peliculas_favoritas}}\r\n  ],\r\n  \"peliculas_vistas\": [\r\n    {{peliculas_vistas}}\r\n  ]\r\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{UrlBase}}/usuarios/crearUsuario",
					"host": [
						"{{UrlBase}}"
					],
					"path": [
						"usuarios",
						"crearUsuario"
					]
				}
			},
			"response": []
		},
